# mesh definition:
mesh unstructured mesh.pmp

# material definition:
material fuel1 ../materials/fuel1.pmp
material fuel2 ../materials/fuel2.pmp
material fuel2+rod ../materials/fuel2+rod.pmp
material reflector ../materials/reflector.pmp

# solver definition:
solver sn {
   energy-groups 2
   order 2
   mixed-face-interpolation 1.0
   least-squares-boundary-interpolation 1
}
